/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    Nand (a=a , b=b, out=Step1);
    Nand (a=a , b=Step1 , out=Step21);
    Nand (a=b , b=Step1 , out=Step22);
    Nand (a=Step21 , b=Step22 , out=out);
}
